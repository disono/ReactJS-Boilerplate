{
  "name": "reactjs-boilerplate",
  "version": "1.0.0",
  "description": "ReactJS Boilerplate is licensed under the Apache License (ASL) license. For more information, see the LICENSE file in this repository.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && xcopy \"src/index.html\" \"dist/index.html\" /F /Y && webpack-dev-server --content-base src/ --inline --hot --history-api-fallback --port 7070",
    "build:prod": "webpack -p && xcopy \"src/index.html\" \"dist/index.html\" /F /Y"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/disono/ReactJS-Boilerplate.git"
  },
  "keywords": [
    "Todo",
    "App",
    "using",
    "ReactJS"
  ],
  "author": "Archie Disono",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/disono/ReactJS-Boilerplate/issues"
  },
  "homepage": "https://github.com/disono/ReactJS-Boilerplate#readme",
  "dependencies": {
    "bulma": "^0.6.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  }
}
